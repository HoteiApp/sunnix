#--------------------------------------------------------------------------|
#-- System configuration file                                              |                                            
#-- Warning: All password variables must be base64 encoded, not plain text |
#--------------------------------------------------------------------------|
# -- Port through which the API is exposed
sunissUp.api.Port = "8080"
sunissUp.api.URL = "https://api.example.com"
sunissUp.api.AllowOrigins="https://test.example.com, https://app.example.com, http://localhost:3000"
sunissUp.rpc.Port = "8081"
sunissUp.Key="example_jwt_key"
sunissUp.SecretKey="YmFzZTY0X2VuY29kZWRfc2VjcmV0"

# -- System Language
sunissUp.Language="es"
sunissUp.Location="Europe/Madrid"
#---------------------------------------------------------------------
# -- AI

sunissUp.ia.apiKey="your-ai-api-key-here"
#---------------------------------------------------------------------
# -- Database type (sqlite|mysql|postgres)
sunissUp.db.type = "mysql"

# -- SQLITE
sunissUp.db.sqlite.name="example.db"

# -- MYSQL
sunissUp.db.mysql.Host="localhost"
sunissUp.db.mysql.Port="3306"
sunissUp.db.mysql.Username="dbuser"
sunissUp.db.mysql.Password="ZXhhbXBsZV9wYXNzd29yZA=="
sunissUp.db.mysql.DB="example_db"

# -- S3
#---------------------------------------------------------------------
sunissUp.s3.USE="aws"
sunissUp.s3.ACCESS_KEY="YOUR_ACCESS_KEY_HERE"
sunissUp.s3.SECRET_KEY="YOUR_SECRET_KEY_HERE"
sunissUp.s3.S3_REGION="eu-west-1"
sunissUp.s3.S3_BUCKET="example-bucket"
sunissUp.s3.ENDPOINT="http://localhost:9000"
sunissUp.s3.CHUNK_SIZE="50"
#---------------------------------------------------------------------

# -- LDAP MASTER
sunissUp.ldap.Host="ldap.example.com"
sunissUp.ldap.Port="389"
sunissUp.ldap.User="cn=admin,dc=example,dc=com"
sunissUp.ldap.Password="example_password"
sunissUp.ldap.BaseDn="dc=example,dc=com"
#---------------------------------------------------------------------

#----------
mode="silence"
#----------